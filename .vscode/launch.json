   "version": "0.2.0",
   "configurations": [
       
      {
         "name": "Python: Flask",
         "type": "python",
         "request": "launch",
         "module": "flask",
         "env": {
            "FLASK_APP": "app.py",
            "FLASK_ENV": "development"
         },
         "args": [
            "run",
            "--no-debugger",
            // "--cert=adhoc"
         ],
         "jinja": true,
         "justMyCode": true
      },
      {
         "name": "Python: Gunicorn",
         "type": "python",
         "request": "launch",
         "module": "gunicorn",
         "env": { 
             "GEVENT_SUPPORT": "True",
             "HOME_AUTO_SIM_TEST": "True",
             "HOME_AUTO_SECRET_KEY": "7fc2b780-6852-4e8c-9332-f869dc940b78",
             "REDIS_URL": "redis://localhost:6379"
         },
         "args": [
            "-w 4",
            "-k gevent",
            "-b 127.0.0.1:5000",
            "wsgi:app"
         ],
         "jinja": true,
         "justMyCode": true
      },
      {
         "name": "Python: Test",
         "type": "python",
         "request": "launch",
         "program": "test.py",
         "env": { 
         },
         "args": [
         ],
         "jinja": true,
         "justMyCode": true,
         "console": "integratedTerminal"
      }
   ]
